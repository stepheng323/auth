    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "file-type": "^16.5.3",
    "firebase-admin": "^10.0.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.5.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
