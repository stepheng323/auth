{
  "name": "moni-track",
  "version": "1.0.0",
  "description": "moni track",
  "main": "app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "migration": "sequelize db:migrate",
    "migration:rollback": "sequelize db:migrate:undo:all",
    "migration:refresh": "npm run migration:rollback && npm run migration",
    "migration:refresh:test": "NODE_ENV=test npm run migration:rollback && NODE_ENV=test npm run migration",
    "db:seed": "sequelize db:seed:all --debug",
    "build": "tsc -p . && npm run migration"
  },
  "author": "",
  "license": "ISC",
